<a href="{{url}}" class="article" style="height: 129px;">
  <div class="side-front">
    {{> logo}}
    <span class="source-name">
      {{displayUrl}}
    </span>
    <p class="title">
      {{title}}
    </p>
  </div>
  <div class="side-back">
    <p class="abstract">
      {{description}}
    </p>
  </div>
</a>
