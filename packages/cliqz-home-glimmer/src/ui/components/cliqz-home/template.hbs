<div id="home">
  <nav id="nav-left">
  </nav>
  <section id="content">
    <section id="top">
      {{#if dials.history}}
        <speed-dial-row @model={{dials.history}} @type="history"></speed-dial-row>
      {{/if}}
      {{#if dials.custom}}
        <speed-dial-row @model={{dials.custom}} @type="custom"></speed-dial-row>
      {{/if}}
    </section>
    <section id="middle">
      {{#if locale}}
        <url-bar @locale={{locale}}></url-bar>
      {{/if}}
    </section>
    <section id="bottom">
      {{#if news}}
        <cliqz-news @model={{news}}></cliqz-news>
      {{/if}}
    </section>
  </section>
  <nav id="nav-right">
  </nav>
</div>